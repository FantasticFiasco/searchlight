{
  "name": "axis-searchlight",
  "version": "0.0.1",
  "description": "A desktop application capable of finding Axis Communication cameras on the network.",
  "keywords": [
    "searchlight",
    "axis",
    "communications",
    "camera",
    "device",
    "discovery",
    "bonjour",
    "ssdp",
    "upnp"
  ],
  "homepage": "https://github.com/FantasticFiasco/axis-searchlight",
  "repository": {
    "type": "git",
    "url": "https://github.com/FantasticFiasco/axis-searchlight.git"
  },
  "bugs": {
    "url": "https://github.com/FantasticFiasco/axis-searchlight/issues"
  },
  "author": "Mattias Kindborg <mattias.kindborg@gmail.com> (https://twitter.com/FantasticFiasco)",
  "license": "GPL-3.0",
  "main": "./src/main.js",
  "scripts": {
    "build": "tsc",
    "prestart": "yarn build",
    "start": "electron .",
    "lint": "tslint \"./src/**/*.ts\"",
    "analytics": "json -I -f ./src/analytics/config.json -e this.trackingId='process.env.UNIVERSAL_ANALYTICS_TRACKING_ID'",
    "prerelease": "yarn analytics && yarn build",
    "release": "electron-builder"
  },
  "dependencies": {
    "@fantasticfiasco/expect": "1.0.0",
    "electron-log": "2.2.7",
    "electron-store": "1.2.0",
    "electron-updater": "2.8.2",
    "universal-analytics": "0.4.13",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@types/electron-store": "1.2.0",
    "@types/universal-analytics": "0.4.0",
    "@types/uuid": "3.4.0",
    "electron": "1.6.11",
    "electron-builder": "19.18.1",
    "json": "9.0.6",
    "tslint": "5.5.0",
    "typescript": "2.4.2"
  }
}